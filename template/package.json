{
  "name": "WebgevMobile",
  "version": "0.0.1",
  "private": true,
  "author": "Webgev <webgev@gmail.com>",
  "scripts": {
    "android": "ENVFILE=.env react-native run-android",
    "ios": "ENVFILE=.env react-native run-ios ",
    "start": "react-native start",
    "pod": "npx pod-install",
    "doctor": "react-native doctor",
    "test": "jest",
    "lint": "eslint .",
    "ts:check": "tsc --watch",
    "ts:check:ci": "tsc",
    "extract:lang": "node tools/i18n/extractMessages.js > src/lib/translate/translates/en.json",
    "lang:sort": "node tools/i18n/sortFile.js src/lib/translate/translates",
    "gen": "graphql-codegen --config codegen.yml",
    "apk": "ENVFILE=.env react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle && cd ./android && ./gradlew app:assembleRelease",
    "aab": "ENVFILE=.env react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle && cd ./android && ./gradlew app:bundleRelease"
  },
  "dependencies": {
    "@apollo/client": "^3.6.6",
    "@formatjs/intl-locale": "^3.3.2",
    "@formatjs/intl-pluralrules": "^5.2.4",
    "@react-native-async-storage/async-storage": "^1.19.1",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@sentry/react-native": "^4.1.0",
    "@webgev/react-native-config": "^0.2.0",
    "@webgev/react-native-size": "^0.2.2",
    "apollo3-cache-persist": "^0.14.0",
    "date-fns": "^2.28.0",
    "formik": "^2.2.9",
    "fuse.js": "^6.6.2",
    "graphql": "^16.5.0",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-intl": "^6.4.4",
    "react-native": "0.72.3",
    "react-native-bootsplash": "^4.3.2",
    "react-native-config": "^1.4.6",
    "react-native-device-info": "^10.8.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-flash-message": "^0.3.1",
    "react-native-localize": "^3.0.2",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.22.1",
    "react-native-secure-key-store": "^2.0.10",
    "react-native-svg": "^13.10.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript-operations": "^2.4.1",
    "@graphql-codegen/typescript-react-apollo": "^3.2.15",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.9",
    "@sentry/types": "^7.5.1",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-formatjs": "^10.5.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.19.0",
    "eslint-plugin-import": "^2.25.3",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
